<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="ru.ancientempires"
	android:installLocation="preferExternal"
	android:sharedUserId="ru.ancientempires"
	android:versionCode="1"
	android:versionName="1.0" >

	<uses-sdk
		android:minSdkVersion="16"
		android:targetSdkVersion="20" />

	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme" >
		<activity
			android:name="ru.ancientempires.MainActivity1"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="ru.ancientempires.gamelife.GameLifeStartActivity"
			android:label="@string/title_activity_game_life"
			android:parentActivityName="ru.ancientempires.MainActivity" >
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value="ru.ancientempires.MainActivity" />
		</activity>
		<activity
			android:name="ru.ancientempires.gamelife.GameLifeActivity"
			android:label="@string/title_activity_game_life"
			android:parentActivityName="ru.ancientempires.gamelife.GameLifeStartActivity" >
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value="ru.ancientempires.gamelife.GameLifeStartActivity" />
		</activity>
		<activity
			android:name="ru.ancientempires.GameActivity"
			android:label="@string/title_activity_game" >
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value="ru.ancientempires.MainActivity" />
		</activity>
	</application>

</manifest>